<h1>Books</h1>
<table>
<tr>
    <th>Title</th>
    <th>Body</th>
    <th></th>
</tr>
<% @books.each do |book| %>
<tr>
    <td><%= book.title %></td>
    <td><%= book.body %></td>
</tr>
<% end %>
</table>

<%= form_with model: @book do |f| %>
　<h2> New book</h2>
　<p>Title</p>
　<%= f.text_field :title %>
　
　<p>Body</p>
　<%= f.text_area :body %>
　
　<%= f.submit 'Create Book' %>
<% end %>